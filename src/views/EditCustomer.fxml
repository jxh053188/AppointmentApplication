<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EditCustomerController">
   <children>
      <Label fx:id="titleLabel" layoutX="120.0" layoutY="29.0" text="Edit Customer">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="customerIdLabel" alignment="CENTER" layoutX="150.0" layoutY="121.0" text="CustomerID" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="customerNameLabel" alignment="CENTER" layoutX="151.0" layoutY="183.0" text="Customer Name" AnchorPane.leftAnchor="151.0" AnchorPane.rightAnchor="149.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="phoneNumberLabel" alignment="CENTER" layoutX="150.0" layoutY="250.0" text="Phone Number" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="addressLabel" alignment="CENTER" layoutX="150.0" layoutY="315.0" text="Street Address"
             AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
          <font>
              <Font size="14.0"/>
          </font>
      </Label>
      <Label fx:id="postCodeLabel" alignment="CENTER" layoutX="150.0" layoutY="384.0" text="Postal Code" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="countryLabel" alignment="CENTER" layoutX="150.0" layoutY="450.0" text="Country" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="stateLabel" alignment="CENTER" layoutX="150.0" layoutY="511.0" text="State" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="customerIdTextField" disable="true" editable="false" layoutX="157.0" layoutY="149.0" />
      <TextField fx:id="customerNameTextField" layoutX="157.0" layoutY="213.0" />
      <TextField fx:id="phoneNumberTextField" layoutX="157.0" layoutY="279.0" />
      <TextField fx:id="addressTextField" layoutX="157.0" layoutY="342.0" />
      <TextField fx:id="postalCodeTextField" layoutX="157.0" layoutY="412.0" />
      <ComboBox fx:id="countryComboBox" layoutX="157.0" layoutY="479.0" onAction="#onSelectCountry" prefWidth="150.0" />
      <ComboBox fx:id="stateComboBox" layoutX="157.0" layoutY="540.0" prefWidth="150.0" />
      <Button fx:id="submitButton" layoutX="113.0" layoutY="625.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="43.0" prefWidth="89.0" text="Submit" AnchorPane.leftAnchor="113.0" />
      <Button fx:id="cancelButton" layoutX="279.0" layoutY="625.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="43.0" prefWidth="89.0" text="Cancel" AnchorPane.rightAnchor="87.0" />
   </children>
</AnchorPane>
